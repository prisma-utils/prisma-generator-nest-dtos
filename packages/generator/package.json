{
  "name": "@prisma-utils/prisma-generator-nest-dtos",
  "description": "Automagically create CreateDTOs and UpdateDTOs based on the Prisma Schema",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-nest-dtos": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.9.1",
    "@prisma/generator-helper": "4.10.0",
    "@prisma/sdk": "3.9.2",
    "prettier": "2.5.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "2.4.2",
    "jest": "27.4.7",
    "prisma": "3.9.2",
    "ts-jest": "27.1.2",
    "typescript": "4.6.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^18.0.1"
  },
  "homepage": "https://github.com/prisma-utils/prisma-generator-nest-dtos",
  "repository": {
    "type": "git",
    "url": "https://github.com/prisma-utils/prisma-generator-nest-dtos"
  },
  "author": {
    "name": "Johannes Schobel",
    "url": "https://github.com/johannesschobel"
  },
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
